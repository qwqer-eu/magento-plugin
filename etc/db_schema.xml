<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="quote_address" resource="default" engine="innodb" comment="Quote Address">
        <column xsi:type="varchar" name="qwqer_address" nullable="true" length="255" comment="qwqer address"/>
    </table>
    <table name="sales_order" resource="sales" engine="innodb" comment="Salel Order">
        <column xsi:type="text" name="qwqer_data" nullable="true"
                comment="QWQER Order Data"/>
        <column xsi:type="text" name="qwqer_order_id" nullable="true"
                comment="QWQER Order Id"/>
    </table>
</schema>
